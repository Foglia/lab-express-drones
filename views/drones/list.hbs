<h2>Available drones</h2>
{{#each allDrones}}
<div>
    <ul>
        <li>{{name}}</li>
        <li>{{propellers}}</li>
        <li>{{maxSpeed}}</li>
        <button type="submit"><a href="/drones/{{_id}}/edit">Edit Drone</a></button>
        <form action="/drones/{{_id}}/delete" method="post">  
        <button type="submit">Delete Drone</button>
        </form>
    <ul>
</div>
{{/each}}